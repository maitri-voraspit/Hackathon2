<%- include ("partials/header") %>

<div class="container">

<% projects.forEach(function(project){ %>
	<div class="card mb-3" style="max-width: 540px;">
	  <div class="row no-gutters">
		<div class="col-md-4">
		  <img src="<%= project.image %>" class="card-img" alt="...">
		</div>
		  
		<div class="col-md-8">
			<div>
			  Tag- <%=project.genre%>
		  </div>
		  <div class="card-body">
			<h5 class="card-title"><%=project.topic %></h5>
			<p class="card-text">Click More Info to know more about the issue</p>
			<a href="/dashboard/<%= project._id %>" class="btn btn-primary">More Info</a>
		  </div>
		</div>
	  </div>
	</div>
<% }); %>
	
</div>

<%- include ("partials/footer") %>